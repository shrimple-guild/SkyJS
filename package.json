{
	"name": "skyjs",
	"type": "module",
	"version": "1.0.0",
	"description": "An API wrapper for Hypixel Skyblock.",
	"main": "dist/app.js",
	"scripts": {
		"dev": "env-cmd --silent nodemon ./src/app.ts",
		"start": "node --env-file .env --enable-source-maps .",
		"prestart": "tsc --build",
		"format": "pretty-quick",
		"pretest": "tsc --build",
		"test": "node --env-file test.env --test ./dist/tests/*.test.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/shrimple-guild/SkyJS/"
	},
	"author": "appable",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/shrimple-guild/SkyJS/issues"
	},
	"trustedDependencies": [
		"sharp"
	],
	"dependencies": {
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"ioredis": "^5.3.2",
		"memoizee": "^0.4.15",
		"p-queue": "^8.0.1",
		"pg": "^8.11.3",
		"sharp": "0.32.6"
	},
	"devDependencies": {
		"@types/express": "^4.17.21",
		"@types/memoizee": "^0.4.9",
		"@types/node": "^18.15.11",
		"@types/pg": "^8.10.9",
		"env-cmd": "^10.1.0",
		"nodemon": "^2.0.22",
		"prettier": "^2.8.7",
		"pretty-quick": "^3.1.3",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.4"
	},
	"volta": {
		"node": "18.15.0",
		"yarn": "1.22.19"
	}
}
